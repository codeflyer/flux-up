{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "phantomjs": true
  },
  "globals": {
    "__DEV__": true,
    "browser": true,
    "element":true,
    "by":true
  },
  "rules": {
    "space-before-blocks": 2,
    "indent": [
      2,
      2,
      {"indentSwitchCase": true}
    ],
    "brace-style": 2,
    "space-after-keywords": 2,
    "strict": 2,
    "no-comma-dangle": 2,
    "no-unused-expressions": 2,
    "block-scoped-var": 2,
    "eol-last": 2,
    "dot-notation": 2,
    "consistent-return": 2,
    "no-unused-vars": [
      2,
      {"args": []}
    ],
    "quotes": [
      2,
      "single"
    ],
    "max-len": [
      1,
      120
    ],
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "no-use-before-define": 0,
    "key-spacing": 0,
    "no-multi-spaces": 0
  }
}